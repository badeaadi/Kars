        android:theme="@style/AppTheme">

    <meta-data
        android:name="com.google.android.gms.ads.APPLICATION_ID"
        android:value="ca-app-pub-2583312812598957~3070270602"/>

        <activity android:name=".SaveCar">


        </activity>

